<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/loginpagina"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="#CB007C"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/settingstoggle3"
        android:layout_width="30dp"
        android:layout_height="29dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.976"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.004"
        app:srcCompat="@drawable/frame_12__1_" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="392dp"
        android:layout_height="92dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.485"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bobdebakker_1" />

    <ImageView
        android:id="@+id/menutoggle3"
        android:layout_width="24dp"
        android:layout_height="27dp"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.025"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.007"
        app:srcCompat="@drawable/frame_12" />

    <ImageView
        android:id="@+id/menutoggle5"
        android:layout_width="24dp"
        android:layout_height="27dp"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.025"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.007"
        app:srcCompat="@drawable/frame_12" />

    <ScrollView
        android:layout_width="410dp"
        android:layout_height="614dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        tools:layout_editor_absoluteX="0dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Moi eem bart," />

            <Button
                android:id="@+id/Bigaccountbutton"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#CB007C"
                android:text="Account" />

            <Button
                android:id="@+id/Bigbroodbutton"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#CB007C"
                android:text="Broden" />

            <ImageButton
                android:id="@+id/Buitenkansjes"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/fuck_die_chineze_kutkinderen__1_" />

            <ImageButton
                android:id="@+id/broodjegezond"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/broodje_gezond__1_" />

            <ImageButton
                android:id="@+id/broodjetonijnsalade"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/broodje_tonijnsalade__1_" />

            <ImageButton
                android:id="@+id/broodjezalm"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/zalmbroodje__1_" />

        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>